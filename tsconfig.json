{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "./src/types.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@posts": [
        "src/pages/posts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}